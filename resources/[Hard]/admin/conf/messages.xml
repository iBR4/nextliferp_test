<messages>
  <!-- Accepted subgroups:
            all - Output to chatbox for all
            log - Log file output
            player - Victim/Player chatbox output
            admin - Admin chatbox output
       Variables:
            $player - player
            $admin - admin
            $data - Additional information, not all the nodes use it.
            $data2 - Additional information, not all the nodes use it.
    -->
  <player>
    <group action="ban" r="255" g="0" b="0">
      <all>$player ha sido baneado$by_admin_4all. $data2</all>
      <log>ADMIN: $admin has banned $player $data $data2</log>
    </group>
    <group action="kick" r="255" g="0" b="0">
      <all>$player ha sido kickeado$by_admin_4all.</all>
      <log>ADMIN: $admin has kicked $player $data</log>
    </group>
    <group action="mute" r="255" g="0" b="0">
      <all>$player ha sido muteado$by_admin_4all. $data2</all>
      <log>ADMIN: $admin has muted $player $data $data2</log>
    </group>
	<group action="setnick" r="255" g="0" b="0">
      <log>ADMIN: $player name changed to $data2 $by_admin_4all.</log>
    </group>
    <group action="unmute" r="0" g="255" b="100">
      <all>$player ha sido desmuteado$by_admin_4all.</all>
      <log>ADMIN: $admin has unmuted $player</log>
    </group>
    <group action="freeze" r="255" g="0" b="0">
      <admin>$player has been frozen$by_admin_4all.</admin>
      <player>Has sido congelado$by_admin_4plr.</player>
      <log>ADMIN: $admin has frozen $player</log>
    </group>
    <group action="unfreeze" r="0" g="255" b="100">
      <admin>$player has been unfrozen$by_admin_4all.</admin>
      <player>Has sido descongelado$by_admin_4plr.</player>
      <log>ADMIN: $admin has unfrozen $player</log>
    </group>
    <group action="slap" r="235" g="20" b="200">
      <all>$player ha sido golpeado$by_admin_4all. ($data HP)</all>
      <log>ADMIN: $admin has slapped $player ($data HP)</log>
    </group>
    <group action="shout" r="255" g="100" b="30">
      <admin>Le has gritado a$player</admin>
    </group>
    <group action="sethealth" r="0" g="170" b="170">
      <admin>$player, su salud ha cambiado a $data</admin>
      <player>Tu salud ha sido cambiada a $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has changed $player's health to $data</log>
    </group>
    <group action="setarmour" r="122" g="122" b="122">
      <admin>$player, su armadura fue cambiada a $data</admin>
      <player>Tu armadura fue cambiada a $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has changed $player's armour to $data</log>
    </group>
    <group action="setmoney" r="225" g="170" b="90">
      <admin>$player, su cantidad de dinero ahora es $data</admin>
      <player>Tu cantidad de dinero ha sido cambiada a $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has set $player's money to $data</log>
    </group>
    <group action="setskin" r="255" g="122" b="0">
      <admin>$player, su skin cambió al # $data</admin>
      <player>Tu skin fue cambiado al # $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has changed $player's skin to $data</log>
    </group>
    <group action="setteam" r="255" g="255" b="0">
      <admin>$player ha sido movido al team $data</admin>
      <player>Has sido movido al team $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has moved $player to the $data team</log>
    </group>
	<group action="removefromteam" r="255" g="255" b="0">
      <admin>$player fue echado del team $data</admin>
      <player>Has sido echado del team $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has removed $player from the $data team</log>
    </group>
    <group action="setinterior" r="0" g="255" b="0">
      <admin>$player ha sido movido al interior $data .</admin>
      <player>Has sido movido al interior $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has moved $player to the $data interior</log>
    </group>
    <group action="setdimension" r="0" g="255" b="0">
      <admin>$player ha sido movido a la dimensión # $data .</admin>
      <player>Has sido movido a la dimensión # $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has moved $player to the $data dimension</log>
    </group>
    <group action="setstat" r="255" g="122" b="0">
      <admin>$player: $data ha sido modificado/a a $data2</admin>
      <player>Tu $data ha sido cambiada a $data2$by_admin_4plr.</player>
      <log>ADMIN: $admin has changed $player's $data to $data2</log>
    </group>
    <group action="jetpackr" r="255" g="0" b="0">
      <admin>$player ha sido despojado de su jetpack</admin>
      <player>Tu jetpack ha sido removido$by_admin_4plr.</player>
      <log>ADMIN: $admin has removed $player's jetpack</log>
    </group>
    <group action="jetpacka" r="255" g="122" b="0">
      <admin>$player ha obtenido un jetpack</admin>
      <player>Se te ha otorgado un jetpack$by_admin_4plr.</player>
      <log>ADMIN: $admin has given a jetpack to $player</log>
    </group>
    <group action="adminr" r="255" g="0" b="0">
      <admin>$player ya no posee derechos de administrador</admin>
      <player>Tus derechos de administrador han sido anulados$by_admin_4plr.</player>
      <log>ADMIN: $admin has revoked admin privilegies from $player</log>
    </group>
    <group action="admina" r="0" g="255" b="0">
      <admin>$player ha obtenido derechos de administrador</admin>
      <player>Se te han otorgado derechos de administrador$by_admin_4plr.</player>
      <log>ADMIN: $admin has given admin privilegies to $player</log>
    </group>
    <group action="givevehicle" r="0" g="255" b="0">
      <admin>$player ha obtenido '$data'</admin>
      <player>Se te ha otorgado '$data'$by_admin_4plr.</player>
      <log>ADMIN: $admin has given $player a '$data'" </log>
    </group>
    <group action="giveweapon" r="0" g="0" b="255">
      <admin>$player ha obtenido '$data'($data2)</admin>
      <player>Se te ha otorgado '$data'($data2)$by_admin_4plr.</player>
      <log>ADMIN: $admin has given $player '$data'</log>
    </group>
    <group action="warp" r="0" g="200" b="50">
      <admin>Te has teletransportado a $player</admin>
      <log>ADMIN: $admin has warped to $player</log>
    </group>
    <group action="warpto" r="0" g="200" b="50">
      <admin>$player ha sido teletransportado a $data</admin>
      <player>Has sido teletransportado a $data$by_admin_4plr.</player>
      <log>ADMIN: $admin has moved $player to $data</log>
    </group>
  </player>
  <team>
    <group action="createteam" r="225" g="170" b="90">
      <admin>Exitosamente creaste el team '$data'</admin>
      <log>ADMIN: $admin has created a team '$data'</log>
    </group>
    <group action="destroyteam" r="225" g="170" b="90">
      <admin>Exitosamente destruiste el team '$data'</admin>
      <log>ADMIN: $admin has destroyed a team '$data'</log>
    </group>
  </team>
  <vehicle>
    <group action="repair" r="0" g="255" b="0">
      <admin>$player: su vehículo fue reparado.</admin>
      <player>Tu vehículo fue reparado$by_admin_4plr.</player>
